{
    "$schema": "https://json.schemastore.org/web-manifest-combined.json",
    "id": "astro-pwa-recipe",
    "name": "Astro PWA | The recipe",
    "short_name": "Astro PWA",
    "description": "Example PWA for Astro using Vite-PWA",
    "orientation": "portrait",
    "display": "standalone",
    "theme_color": "#ff5d01",
    "background_color": "#11191f",
    "icons": [
      {
        "src": "pwa-64x64.png",
        "sizes": "64x64",
        "type": "image/png"
      },
      {
        "src": "pwa-192x192.png",
        "sizes": "192x192",
        "type": "image/png"
      },
      {
        "src": "pwa-512x512.png",
        "sizes": "512x512",
        "type": "image/png",
        "purpose": "any"
      },
      {
        "src": "maskable-icon-512x512.png",
        "sizes": "512x512",
        "type": "image/png",
        "purpose": "maskable"
      }
    ]
  }