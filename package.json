{
  "name": "docs",
  "version": "0.0.7",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "check": "astro check",
    "format": "pnpm run format:code",
    "format:ci": "pnpm run format:imports && pnpm run format:code",
    "format:code": "prettier -w . --cache --plugin-search-dir=.",
    "format:imports": "echo 'Skipped formatting imports'",
    "docgen": "node ./scripts/docgen.mjs",
    "docgen:errors": "node ./scripts/error-docgen.mjs",
    "lint:linkcheck": "SKIP_OG=true pnpm build && tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/lint-linkcheck.ts",
    "lint:linkcheck:nobuild": "tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/lint-linkcheck.ts",
    "lint:slugcheck": "node ./scripts/lint-slugcheck.mjs",
    "lint:eslint": "eslint .",
    "lint:ja": "textlint --cache --config .textlintrc.ja.json src/content/docs/ja/**/*.{md,mdx}",
    "netlify:build": "pnpm ${NETLIFY_BUILD_SCRIPT:-build}",
    "lunaria:build": "tsm ./scripts/lunaria.mts"
  },
  "devDependencies": {
    "@11ty/eleventy-fetch": "^5.0.1",
    "@actions/core": "^1.11.1",
    "@docsearch/css": "3.5.1",
    "@docsearch/js": "3.5.1",
    "@eslint/js": "^9.17.0",
    "@types/canvas-confetti": "^1.6.0",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.17.10",
    "@typescript-eslint/parser": "^8.19.1",
    "astro": "^5.13.5",
    "astro-auto-import": "^0.4.4",
    "astro-eslint-parser": "^1.2.2",
    "astro-og-canvas": "^0.7.0",
    "canvaskit-wasm": "^0.39.1",
    "dedent-js": "^1.0.1",
    "domhandler": "^5.0.3",
    "eslint": "^9.17.0",
    "eslint-plugin-astro": "^1.3.1",
    "fast-glob": "^3.3.3",
    "globals": "^15.14.0",
    "hast-util-select": "^6.0.4",
    "hastscript": "^9.0.1",
    "htmlparser2": "^9.1.0",
    "kleur": "^4.1.5",
    "mdast-util-to-string": "^4.0.0",
    "p-retry": "^6.2.1",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "rehype": "^13.0.2",
    "textlint": "14.7.2",
    "textlint-plugin-mdx": "1.0.2",
    "textlint-rule-preset-ja-spacing": "2.4.3",
    "tsm": "^2.3.0",
    "typescript": "5.6.2",
    "typescript-eslint": "^8.19.1",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/sitemap": "^3.5.1",
    "@astrojs/starlight": "^0.35.2",
    "@expressive-code/plugin-collapsible-sections": "^0.41.3",
    "@lunariajs/core": "https://pkg.pr.new/lunariajs/lunaria/@lunariajs/core@f07e1a3",
    "canvas-confetti": "^1.6.0",
    "jsdoc-api": "^9.3.4",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "remark-smartypants": "^2.0.0",
    "sharp": "^0.33.5",
    "starlight-llms-txt": "^0.5.1"
  },
  "packageManager": "pnpm@9.15.1",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom",
        "vite"
      ]
    }
  }
}
